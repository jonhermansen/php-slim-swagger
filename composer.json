{
  "name": "mijohansen/slim-swagger",
  "description": "Simpler way to hook a swagger api into slim-framework.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Michael Johansen",
      "email": "michael.johansen@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "psx/model": "^2.0",
    "slim/slim": "^3.10",
    "psx/schema": "^2.4",
    "composer/spdx-licenses": "^1.4",
    "doctrine/annotations": "1.4",
    "eloquent/composer-config-reader": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "vierbergenlars/php-semver": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "SlimSwagger\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": "mkdir -p ./vendor/vierbergenlars/php-semver/vendor; echo '<?php require __DIR__.\"/../../../autoload.php\";' > ./vendor/vierbergenlars/php-semver/vendor/autoload.php"
  }


}
